{% if product.metafields.custom_fields["bp_icon"] != blank %}
  <div class="custom-field custom-field__bp-icon custom-field__type--image">
    <h3 class="custom-field--title">Icon</h3>
    <div class="custom-field--value">  
      {% render 'cf-image' with image: product.metafields.custom_fields["bp_icon"], print: 'url' %}
    </div>
  </div>
{% endif %}